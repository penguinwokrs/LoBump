{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types", "node"],
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true
	},
	"include": ["**/*.ts"]
}
